pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
timers = {
	global = 0
}

camx = 0
camy = 0
shadows = {
	starttxt_x = 0,
	starttxt_y = 0
}

heli = {
	x = 64,
	y = 64,
	sprite = 0,
	shadow_spr = 32,
	crash = 0,
	score = 0,
	animate = function(self)
		self.sprite += 1
		self.shadow_spr += 1
		if self.sprite == 2 then self.sprite = 0 end
		if self.shadow_spr == 34 then self.shadow_spr = 32 end
	end,
	blasts = {}
}

enemies = {}

function ceil(num)
	return -flr(-num)
end

--start screen
function update_start()
	cls()
	timers.global += 1
	if timers.global % 12 == 0 then
		if shadows.starttxt_x < 8 then
			shadows.starttxt_x += 1
			shadows.starttxt_y = shadows.starttxt_x
		else
			shadows.starttxt_x -= 1
			shadows.starttxt_y = shadows.starttxt_x
		end
	end
	
	shadow = {
		x = 50 + shadows.starttxt_x,
		y = 30 + shadows.starttxt_y
	}
	
	if btn(5) then
		start_game()
	end
end

function draw_start()
 cls()
	rectfill(0,0,128,128,3)
	print("h.e.l.i",shadow.x,shadow.y,5)
	print("h.e.l.i",50,30,4)
	print("[x] start [z] fire",30,60,7)
end

--game
function start_game()
	cls()
	timers.global = 0
 camera()
	sfx(1,1)
	music(0,100,10)
	heli.x = 64
	heli.y = 64
	heli.crash = 0
	heli.blasts = {}
	enemies = {}
	_update = update_game
	_draw = draw_game
end

function update_game()
	if btn(0) then heli.x -=1 end
	if btn(1) then heli.x +=1 end
	if btn(2) then heli.y -=1 end
	if btn(3) then heli.y +=1 end
	if heli.x < -2 then heli.x = -2 end
	if heli.x > 121 then heli.x = 121 end
	if heli.y < 0 then heli.y = 0 end
	if heli.y > 120 then heli.y = 120 end
	
	if btnp(4) then make_heli_blast() end
	
	for blast in all(heli.blasts) do
		if blast.y < 1 then
			del(heli.blasts,blast)
		end
		blast:update()
	end
	
	for enemy in all(enemies) do
		if enemy.y > 127 then
			del(enemies,enemy)
		end
	end
	
	if heli.crash == 1 then
		show_game_over()
	end
	timers.global += 1	
	if count(enemies) < 3 then
		make_enemy()
	end
end

function draw_game()
 cls()
	rectfill(0,0,128,128,3)
 camera()
	print("score: "..heli.score,5,5,6)
	heli:animate()
	--shadow
	pal(4,5)
	pal(7,5)
	spr(heli.sprite, heli.x + 5, heli.y + 6)
	pal()
	--heli
	spr(heli.sprite, heli.x, heli.y)
	
	for blast in all(heli.blasts) do
		blast:draw()
	end
	
	for enemy in all(enemies) do
		enemy:animate()
		enemy:check_collision(heli)
		--shadow
		pal(4,5)
		pal(7,5)
		spr(enemy.sprite, enemy.x + 5, enemy.y + 6,1,1,false,true)
		pal()
		--enemy heli
		pal(4,2)
		spr(enemy.sprite,enemy.x,enemy.y,1,1,false,true)
		pal()
	end
end

function make_heli_blast()
	sfx(0)
	blast = {
		x = heli.x+1,
		y = heli.y,
		update = function(self)
			self.y -= 2
			for enemy in all(enemies) do
				xdiff = enemy.x - self.x
				ydiff = self.y - enemy.y
				if xdiff < 2
							and xdiff > -4
							and ydiff < 1 then
					sfx(3)
					heli.score += ceil(enemy.speed)
					del(enemies,enemy)
				end
			end
		end,
		draw = function(self)
			spr(2, self.x, self.y)
		end
	}
	
	add(heli.blasts, blast)
end

function make_enemy()
	enemy = {
		x = flr(rnd(16)) * 8,
		y = 0,
		speed = 0.25 + rnd(2),
		sprite = 0,
		shadow_spr = 32,
		animate = function(self)
			self.sprite += 1
			self.shadow_spr += 1
			if self.sprite == 2 then self.sprite = 0 end
			if self.shadow_spr == 34 then self.shadow_spr = 32 end
			self.y += self.speed
		end,
		check_collision = function(self,opp)
			local xdiff = self.x - opp.x
			local ydiff = opp.y - self.y

			if xdiff < 5 and xdiff > -5 then
				if ydiff < 8 and ydiff > -8 then
					opp.crash = 1
				end
			end
		end
	}
	add(enemies, enemy)
end

function show_game_over()
		sfx(2,1)
		_update = update_game_over
		_draw = draw_game_over
end

--game over
function update_game_over()
	if btn(5) then start_game()  end
end

function draw_game_over()
	print("game over", 45, 50, 9)
	print("press x to restart",30,58,12)
end

function _init()
_update = update_start
_draw = draw_start
end
