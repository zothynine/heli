pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
timer = 0;

heli = {
	x = 64,
	y = 64,
	sprite = 0,
	crash = 0,
	animate = function(self)
		self.sprite += 1
		if self.sprite == 2 then self.sprite = 0 end
	end,
	blasts = {}
}

enemies = {}

--start screen
function update_start()
	cls()
	timer = 0;
	if btn(4) then
		start_game()
	end
end

function draw_start()
	rectfill(0,0,127,127,5)
	print("press z to start",30,30,12)
end

--game
function start_game()
	cls()
	sfx(1,1)
	heli.x = 64
	heli.y = 64
	heli.crash = 0
	heli.blasts = {}
	enemies = {}
	_update = update_game
	_draw = draw_game
	
	make_enemy()
end

function update_game()
	if btn(0) then heli.x -=1 end
	if btn(1) then heli.x +=1 end
	if btn(2) then heli.y -=1 end
	if btn(3) then heli.y +=1 end
	if heli.x < -2 then heli.x = -2 end
	if heli.x > 121 then heli.x = 121 end
	if heli.y < 0 then heli.y = 0 end
	if heli.y > 120 then heli.y = 120 end
	
	if btnp(4) then make_heli_blast() end
	
	for blast in all(heli.blasts) do
		if blast.y < 1 then
			del(heli.blasts,blast)
		end
		blast:update()
	end
	
	for enemy in all(enemies) do
		if enemy.y > 127 then
			del(enemies,enemy)
		end
	end
	
	if heli.crash == 1 then
		show_game_over()
	end
	timer += 1
end

function draw_game()
	rectfill(0,0,127,127,5)
	print(timer,5,5,6)
	heli:animate()
	spr(heli.sprite, heli.x, heli.y)
	
	for blast in all(heli.blasts) do
		blast:draw()
	end
	
	for enemy in all(enemies) do
		enemy:animate()
		enemy:check_collision(heli)
		spr(enemy.sprite,enemy.x,enemy.y)
	end
end

function make_heli_blast()
	sfx(0)
	blast = {
		x = heli.x+1,
		y = heli.y,
		update = function(self)
			self.y -= 2
			for enemy in all(enemies) do
				xdiff = enemy.x - self.x
				ydiff = self.y - enemy.y
				if xdiff < 2
							and xdiff > -4
							and ydiff < 1 then
					sfx(3)
					del(enemies,enemy)
				end
			end
		end,
		draw = function(self)
			spr(2, self.x, self.y)
		end
	}
	
	add(heli.blasts, blast)
end

function make_enemy()
	enemy = {
		x = 64,
		y = 0,
		sprite = 16,
		animate = function(self)
			self.sprite += 1
			if self.sprite == 18 then self.sprite = 16 end
		end,
		check_collision = function(self,opp)
			local xdiff = self.x - opp.x
			local ydiff = opp.y - self.y

			if (xdiff < 5 and xdiff > -5) then
				if ydiff < 8 then
					opp.crash = 1
				end
			end
		end
	}
	add(enemies, enemy)
end

function show_game_over()
		sfx(2,1)
		_update = update_game_over
		_draw = draw_game_over
end

--game over
function update_game_over()
	if btn(4) then start_game()  end
end

function draw_game_over()
	print("game over", 45, 50, 9)
	print("press z to restart",30,58,12)
end

function _init()
_update = update_start
_draw = draw_start
end
